{
    "name": "jalexisc/Faker",
    "type": "library",
    "description": "Faker is a PHP library that generates fake data for you.",
    "keywords": [
        "faker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Alexis Correa Valencia"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^2.9.2"
    },
    "autoload": {
        "psr-4": {
            "Faker\\": "src/Faker/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
